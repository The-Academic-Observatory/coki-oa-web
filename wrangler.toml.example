name = "coki-oa-web"
type = "webpack"
account_id = "my-cloudflare-account-id"
zone_id = "my-cloudflare-zone-id"
compatibility_date = "2022-02-03"

[site]
bucket = "out"
entry-point = "workers-site"

[env.develop]
workers_dev = true
vars = { ANALYTICS_ENABLED = false }

[env.staging]
workers_dev = true
vars = { ANALYTICS_ENABLED = false }

[env.production]
route = "my-production-domain/*"
vars = { ANALYTICS_ENABLED = true }